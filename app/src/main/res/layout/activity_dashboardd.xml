<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/drawer_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        tools:context=".MainActivity"
        tools:openDrawer="start">


    <RelativeLayout android:layout_width="match_parent"
                    android:layout_height="70dp"

>
        <ImageView
                android:layout_width="match_parent"
                android:layout_height="285dp"
                app:srcCompat="@drawable/yu"
                android:id="@+id/imageView30"
                />
        <androidx.appcompat.widget.Toolbar
                android:id="@+id/actionbar"
                android:background="@drawable/border"
                android:layout_width="match_parent"
                android:gravity="center_horizontal"
                android:layout_height="70dp"
                app:title="Dashboard"
                android:textAlignment="center"
                app:titleTextAppearance="@style/Toolbar.TitleText"
                app:titleMarginStart="100dp"
                app:titleTextColor="#FFFBFB"
                >
        </androidx.appcompat.widget.Toolbar>


<ScrollView android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="75dp"
            >


    <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/rounded_layout"
            android:elevation="3dp"
           >
        <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:srcCompat="@drawable/contact"
                android:id="@+id/contact_us"
                android:layout_marginTop="512dp"
                android:layout_marginLeft="250dp"/>

        <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/phone"
                android:id="@+id/phone"
                android:layout_marginTop="506dp"
                android:layout_marginLeft="190dp"
                android:visibility="invisible"/>

        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                app:cardCornerRadius="10dp"
                android:layout_height="80dp" android:layout_marginTop="430dp" android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp">
            <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/whatsapp"
                    android:id="@+id/whatsapp"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="160dp"
                    android:visibility="invisible"/>


            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:id="@+id/imageView28" android:layout_marginTop="25dp" android:layout_marginLeft="245dp"
                    tools:ignore="VectorDrawableCompat"/>
            <TextView
                    android:text="Reports"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#8C7E7E"
                    android:id="@+id/textView4" android:layout_marginLeft="65dip"
                    android:layout_marginTop="25dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:fontFamily="@font/anaheim" android:textSize="18sp"/>
            <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    app:srcCompat="@drawable/ic_baseline_signal_cellular_alt_24"
                    android:id="@+id/imageView6" tools:ignore="VectorDrawableCompat" android:layout_marginTop="20dp"
                    android:layout_marginLeft="10dip"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:id="@+id/active_orders"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                app:cardCornerRadius="10dp"
                android:layout_marginTop="130dp">
            <TextView
                    android:text="4"
                    android:layout_width="35dp"
                    android:background="@drawable/rounded_text1"
                    android:layout_height="35dp" android:id="@+id/textView26"
                    android:layout_marginLeft="200dp" android:layout_marginTop="20dp"
                    android:gravity="center"/>
            <ImageView
                    android:layout_width="34dp"
                    android:layout_height="40dp"
                    app:srcCompat="@drawable/img"
                    android:id="@+id/imageView"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="10dip"
                    tools:ignore="DuplicateIds,VectorDrawableCompat"/>
            <TextView
                    android:text="Confirmed/Preparing Orders"
                    android:layout_width="125dp"
                    android:layout_height="wrap_content"
                    android:textColor="#8C7E7E"
                    android:id="@+id/textView"
                    android:layout_marginLeft="65dip"
                    android:layout_marginTop="15dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:fontFamily="@font/anaheim" tools:ignore="DuplicateIds" android:textSize="18sp"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:id="@+id/imageView25" android:layout_marginTop="25dp" android:layout_marginLeft="245dp"
                    tools:ignore="DuplicateIds,VectorDrawableCompat"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="80dp" android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                app:cardCornerRadius="10dp"
                android:layout_marginTop="30dp" android:id="@+id/incoming_orders">
            <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    app:srcCompat="@drawable/ic_baseline_list_alt_24"
                    android:id="@+id/imageView7" android:layout_marginTop="20dp" android:layout_marginLeft="10dp"
                    tools:ignore="VectorDrawableCompat"/>
            <TextView
                    android:text="Incoming Orders"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#8C7E7E"
                    android:id="@+id/textView"
                    android:layout_marginLeft="65dip"
                    android:layout_marginTop="25dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:fontFamily="@font/anaheim" tools:ignore="DuplicateIds" android:textSize="18sp"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:id="@+id/imageView25" android:layout_marginTop="25dp" android:layout_marginLeft="245dp"
                    tools:ignore="DuplicateIds,VectorDrawableCompat"/>
            <TextView
                    android:text="26"
                    android:layout_width="35dp"
                    android:background="@drawable/rounded_text"
                    android:layout_height="35dp" android:id="@+id/textView24"
                    android:layout_marginLeft="200dp" android:layout_marginTop="20dp"
                    android:gravity="center"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:id="@+id/delivered_orders"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                app:cardCornerRadius="10dp"
                android:layout_marginTop="230dp">
            <TextView
                    android:text="10"
                    android:layout_width="35dp"
                    android:background="@drawable/rounded_text2"
                    android:layout_height="35dp" android:id="@+id/textView25"
                    android:layout_marginLeft="200dp" android:layout_marginTop="20dp"
                    android:gravity="center"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:id="@+id/imageView26" android:layout_marginTop="25dp" android:layout_marginLeft="245dp"
                    tools:ignore="VectorDrawableCompat"/>
            <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    app:srcCompat="@drawable/ic_baseline_airport_shuttle_24"
                    android:id="@+id/imageView7"
                    android:layout_marginTop="20dp" android:layout_marginLeft="10dip"
                    tools:ignore="DuplicateIds,VectorDrawableCompat"/>
            <TextView
                    android:text="Delivered Orders"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#8C7E7E"
                    android:id="@+id/textView" android:layout_marginLeft="65dip"
                    android:layout_marginTop="25dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:fontFamily="@font/anaheim" tools:ignore="DuplicateIds" android:textSize="18sp"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:id="@+id/cardview1"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="330dp"
                app:cardCornerRadius="10dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp">


            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:id="@+id/imageView27" android:layout_marginTop="25dp" android:layout_marginLeft="245dp"
                    tools:ignore="VectorDrawableCompat"/>
            <TextView
                    android:text="Product"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#8C7E7E"
                    android:id="@+id/textView2" android:layout_marginLeft="65dip"
                    android:layout_marginTop="25dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:fontFamily="@font/anaheim" android:textSize="18sp"/>
            <ImageView
                    android:layout_width="40dp"
                    android:layout_height="43dp" app:srcCompat="@drawable/pro" android:id="@+id/imageView9"
                    android:layout_marginTop="18dp" android:layout_marginLeft="10dip"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                app:cardCornerRadius="10dp"
                android:layout_height="150dp"
                android:layout_marginBottom="15dp"
                android:layout_marginTop="565dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp">


            <lecho.lib.hellocharts.view.PieChartView
                    android:layout_width="139dp"
                    android:layout_height="160dp"
                    android:layout_marginRight="130dp"
                    android:id="@+id/chart"
            ></lecho.lib.hellocharts.view.PieChartView>

            <TextView
                    android:text="Confrimed Orders"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/textView32"
                    android:layout_marginLeft="170dp" android:layout_marginTop="60dp" android:textSize="12sp"/>
            <TextView
                    android:text="Incoming Orders"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/textView31"
                    android:layout_marginLeft="170dp" android:layout_marginTop="30dp" android:textSize="12sp"/>
            <TextView
                    android:layout_height="12dp"
                    android:background="@drawable/rounded_text1"
                    android:id="@+id/textView29" android:layout_marginLeft="150dp"
                    android:layout_marginTop="60dp" android:layout_width="12dp"/>
            <TextView
                    android:layout_height="12dp"
                    android:background="@drawable/rounded_text"
                    android:id="@+id/textView28" android:layout_marginLeft="150dp"
                    android:layout_marginTop="30dp" android:layout_width="12dp"/>
            <TextView
                    android:layout_height="12dp"
                    android:background="@drawable/rounded_text2"
                    android:id="@+id/textView27" android:layout_marginLeft="150dp"
                    android:layout_marginTop="90dp" android:layout_width="12dp"/>
            <TextView
                    android:text="Delivered Orders"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/textView30"
                    android:layout_marginLeft="170dp" android:layout_marginTop="90dp" android:textSize="12sp"/>

        </androidx.cardview.widget.CardView>
        <TextView
                android:text="Total Orders"
                android:layout_width="wrap_content"
                android:layout_height="20dp" android:id="@+id/textView23"
                android:layout_marginTop="520dp"
                android:layout_marginLeft="30dp" android:textStyle="bold"/>

    </RelativeLayout>




</ScrollView>



    </RelativeLayout>

    <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:elevation="3dp"
            android:id="@+id/fragment"></RelativeLayout>













    <com.google.android.material.navigation.NavigationView
            android:layout_gravity="start"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:id="@+id/nav_bar"
            app:headerLayout="@layout/header"
            app:menu="@menu/main_menu"/>







</androidx.drawerlayout.widget.DrawerLayout>