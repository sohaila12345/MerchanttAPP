<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent" xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools">
    <ImageView
            android:layout_width="match_parent"
            android:layout_height="285dp"
            app:srcCompat="@drawable/yu"
            android:id="@+id/bgimage"/>
    <androidx.appcompat.widget.Toolbar
            android:id="@+id/actionbar"
            android:background="@drawable/border"

            android:layout_width="match_parent"
            android:layout_height="70dp"
            app:title="Delivered Orders"
            app:titleMarginStart="100dp"
            app:titleTextColor="#FFFFFF">
    </androidx.appcompat.widget.Toolbar>
    <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_marginLeft="10dp"
            app:cardCornerRadius="10dp"
            android:layout_marginRight="10dp"
            android:layout_height="match_parent"
            android:layout_marginTop="75dp">

    <androidx.cardview.widget.CardView
            android:id="@+id/delivered_orders"
            android:layout_width="match_parent"
            app:cardCornerRadius="10dp"
            android:layout_height="120dip"
            android:layout_marginLeft="12dip"
            android:background="@drawable/rounded_search"
            android:layout_marginRight="12dip"
            android:layout_marginTop="15dip"
            tools:ignore="MissingConstraints">


        <TextView
                android:layout_width="197dp"
                android:layout_height="wrap_content" android:id="@+id/textView7" android:layout_marginLeft="90dip"
                android:layout_marginTop="40dp"
                android:text="Tehsil Hazro,Distt.  Street Number 25 - Street Number 25"/>
        <ImageView
                android:layout_width="85dip"
                android:layout_height="73dp"
                app:srcCompat="@drawable/oih"
                android:id="@+id/imageView21" android:layout_marginTop="25dp"/>
        <TextView
                android:text="Usman Awan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView34" android:layout_marginLeft="90dip"
                android:layout_marginTop="15dp" android:textStyle="bold"/>
        <TextView
                android:text="Status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView35" android:layout_marginLeft="230dip"
                android:layout_marginTop="10dip"/>
        <TextView
                android:text="Apr 01, 04:52 PM "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView36" android:layout_marginTop="86dip"
                android:layout_marginLeft="90dip" android:textSize="12sp"/>

    </androidx.cardview.widget.CardView>
    </androidx.cardview.widget.CardView>
</RelativeLayout>